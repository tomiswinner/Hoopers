<div id='map'></div>
<%= form_with(url: new_court_path, method: 'get', local: true) do |f|  %>
 <%= f.hidden_field(:center_latitude, value: @center_lat) %>
 <%= f.hidden_field(:center_longitude, value: @center_lng) %>
 <%= f.hidden_field(:address, value: @address) %>
 <%= f.hidden_field(:prefecture_id, value: @prefecture_id) %>
<% end %>
<% @courts.each do |court| %>

 <%= hidden_field_tag(:name, court.name, {class: 'names', id: "#{court.name}_name"}) %>
 <%= hidden_field_tag(:latitude, court.latitude, {class: 'latitudes', id: "#{court.name}_lat"}) %>
 <%= hidden_field_tag(:longitude, court.longitude, {class: 'longitudes', id: "#{court.name}_lng"}) %>
 <%= hidden_field_tag(:path, court_path(id: court.id), {class: 'links', id: "#{court.name}_links"}) %>
<% end %>
